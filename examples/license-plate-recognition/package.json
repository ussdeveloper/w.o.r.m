{
  "name": "worm-license-plate-recognition",
  "version": "1.0.0",
  "description": "WORM Universal Language - License Plate Recognition from USB Camera",
  "main": "license-plate-recognizer.js",
  "scripts": {
    "start": "node license-plate-recognizer.js",
    "install-deps": "npm install opencv4nodejs tesseract.js python-shell",
    "setup-python": "pip install opencv-python numpy pillow pytesseract",
    "setup-tesseract": "echo 'Please install Tesseract OCR from https://github.com/UB-Mannheim/tesseract/wiki'",
    "demo": "node license-plate-recognizer.js"
  },
  "dependencies": {
    "opencv4nodejs": "^5.6.0",
    "tesseract.js": "^4.0.0",
    "python-shell": "^3.0.0"
  },
  "optionalDependencies": {
    "ffi-napi": "^4.0.0",
    "archiver": "^5.3.0"
  },
  "keywords": [
    "worm",
    "universal-language",
    "license-plate",
    "ocr",
    "opencv",
    "computer-vision",
    "multi-language",
    "javascript",
    "python",
    "cpp",
    "golang"
  ],
  "author": "WORM Universal Language",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "worm": {
    "languages": ["javascript", "python", "cpp", "go"],
    "features": [
      "real-time-processing",
      "concurrent-ocr",
      "multi-language-pipeline",
      "usb-camera-support"
    ],
    "requirements": {
      "python": {
        "packages": ["opencv-python", "numpy", "pytesseract", "pillow"],
        "optional": true
      },
      "system": {
        "tesseract": "For OCR functionality",
        "camera": "USB camera or webcam",
        "cpp-compiler": "For performance optimizations"
      }
    }
  }
}